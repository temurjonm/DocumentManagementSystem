{
  "name": "dms-backend",
  "version": "1.0.0",
  "description": "Document Management System - Backend Services",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package:lambda": "bash scripts/package-lambda.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest src/integration/",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "setup:test-infra": "bash scripts/setup-test-infra.sh",
    "cleanup:aws": "bash scripts/cleanup-aws.sh"
  },
  "keywords": [
    "dms",
    "document-management",
    "aws"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.133",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^7.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-eventbridge": "^3.490.0",
    "@aws-sdk/client-kms": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-secrets-manager": "^3.958.0",
    "@aws-sdk/client-sfn": "^3.958.0",
    "@aws-sdk/client-sqs": "^3.958.0",
    "@aws-sdk/client-textract": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.11.3",
    "sharp": "^0.34.5",
    "uuid": "^9.0.1"
  }
}
