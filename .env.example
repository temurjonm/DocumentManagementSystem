# AWS Credentials (Get from AWS Console → IAM → Security Credentials)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1

# Database
DATABASE_URL=postgresql://dms_admin:password@localhost:5432/dms_test

# S3 Storage
S3_BUCKET_NAME=dms-documents-test

# KMS Encryption
KMS_KEY_ID=alias/dms-shared-test

# OpenSearch (Optional - for search tests)
OPENSEARCH_ENDPOINT=https://your-opensearch-endpoint.us-east-1.es.amazonaws.com

# SQS (Optional - for deletion tests)
HARD_DELETE_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/dms-hard-delete-test

# Cognito (Optional - for auth tests)
COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
COGNITO_CLIENT_ID=your_client_id_here

# DynamoDB (Optional - for concurrency tests)
DYNAMODB_TABLE_NAME=dms-concurrency-test
